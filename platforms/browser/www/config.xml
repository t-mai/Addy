<?xml version='1.0' encoding='utf-8'?>
<widget id="addy.co.jp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Addy</name>
    <description>
        Prototype of Addy app.
    </description>
    <author email="t-mai@amida.co.jp" href="http://amida.co.jp">
        Mai Xuan Trang
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
